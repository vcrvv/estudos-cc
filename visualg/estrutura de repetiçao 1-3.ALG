Algoritmo "semnome"

Var
   r, d: real
   c, q: inteiro


Inicio
      c <- 1
      escreva ("quantas vezes vc quer? ")
      leia(q)
      enquanto (c <= q) faca
               escreva(" qual o valor em real? ")
               leia(r)
               d <- r/5
               escreval("o valor convertido é: ", d)
               c <- c + 1
      fimenquanto
      


Fimalgoritmo