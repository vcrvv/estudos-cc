Algoritmo "semnome"
var
c, valor, salto : inteiro
Inicio
      c <- 0
      escreva("quer contar ate quanto? ")
      leia(valor)
      escreva("de quanto voce quer começar? ")
      leia(c)
      escreva("de quanto em quanto? ")
      leia(salto)
      enquanto (c <= valor) faca
          escreval(c)
          c <- c + salto
      fimenquanto
      escreval("terminei de contar")
Fimalgoritmo